import math
bar = float(input('Введіть число х:  '))
def f1(bar1):
    rez = 2/x + math.pow(2.31 + math.e*2*x)
    return (rez)
def f2(bar2):
    rez = arcsin(0.2*x + 0.3)
    return (rez)
def f3(bar3):
    rez = cos(x + 0.4.math.log(fabs(x+0.2)))
    return (rez)
y = 0.0
if  bar1 >= 1:
    y = f1(bar1)
elif -1 < bar2 < 1:
    y = f2(bar2)
else:  bar3 <= -1
    y = f3(bar3)
print(int(y))
