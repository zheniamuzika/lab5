import math
bar = float(input('Введіть число х:  '))
def f1(bar1):
    rez = 2/x + math.pow(2.31 + math.e*2*x)
    return (rez)
def f2(bar2):
    rez = math.arcsin(0.2*x + 0.3)
    return (rez)
def f3(bar3):
    rez = math.cos(x + 0.4.math.log(math.fabs(x+0.2)))
    return (rez)
y = 0.0
if  f1(bar) >= 1:
    y = f1(bar)
elif -1 < bar < 1:
    y = f2(bar)
else:  f3(bar) <= -1
y = f3(bar)
print(int(y))
